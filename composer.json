{
  "name": "andyoung/think-tests",
  "description": "think-tests",
  "type": "think-framework",
  "keywords": [
    "framework",
    "thinkphp",
    "unitTest"
  ],
  "license": "MIT",
  "homepage": "https://github.com/AndyYoungCN/think-tests",
  "authors": [
    {
      "name": "andanyang",
      "email": "1218853253@qq.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "topthink/framework": "5.1.*",
    "topthink/think-helper": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "tests\\": "tests",
      "think\\": "thinkphp/library/think",
      "traits\\": "thinkphp/library/traits"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "8",
    "symfony/dom-crawler": "^5.0"
  },
  "extra": {
    "think-path": "thinkphp"
  },
  "config": {
    "preferred-install": "dist"
  }
}
